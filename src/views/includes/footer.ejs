            <li><a href="<%=Url%>minha-conta"><i class="fas fa-user me-2"></i>Minha conta</a> </li>
            <li><a href="<%=Url%>logout"><i class="fas fa-right-to-bracket me-2"></i>Sair da conta</a> </li>
          <% } else { %>
            <li><a href="<%=Url%>login"><i class="fas fa-right-to-bracket me-2"></i>Entrar</a>  </li>
